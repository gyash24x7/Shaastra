{
	"name": "api",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/api/src",
	"projectType": "application",
	"targets": {
		"keys:generate": {
			"command": "ts-node apps/api/scripts/keys.generate.ts"
		},
		"db:push": {
			"command": "prisma db push --schema=apps/api/prisma/schema.prisma --skip-generate --accept-data-loss"
		},
		"db:generate": {
			"inputs": [
				"{projectRoot}/prisma/schema.prisma"
			],
			"outputs": [
				"{workspaceRoot}/node_modules/@prisma/client/**"
			],
			"command": "prisma generate --schema=apps/api/prisma/schema.prisma"
		},
		"build": {
			"executor": "@nrwl/webpack:webpack",
			"outputs": [
				"{options.outputPath}"
			],
			"options": {
				"target": "node",
				"compiler": "tsc",
				"outputPath": "apps/api/dist",
				"main": "apps/api/src/main.ts",
				"tsConfig": "apps/api/tsconfig.build.json",
				"assets": [
					"apps/api/src/assets"
				]
			},
			"configurations": {
				"production": {
					"optimization": true,
					"extractLicenses": true,
					"inspect": false
				}
			}
		},
		"serve": {
			"executor": "@nrwl/js:node",
			"options": {
				"buildTarget": "api:build",
				"inspect": false
			},
			"configurations": {
				"production": {
					"buildTarget": "api:build:production"
				}
			}
		}
	},
	"tags": [ ]
}
