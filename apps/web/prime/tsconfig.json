{
	"extends": "../../../tsconfig.base.json",
	"files": [ ],
	"compilerOptions": {
		"target": "ESNext",
		"useDefineForClassFields": true,
		"lib": [ "DOM", "DOM.Iterable", "ESNext" ],
		"module": "ESNext",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"noEmit": true,
		"plugins": [ { "name": "next" } ],
		"allowJs": true,
		"incremental": true
	},
	"include": [ "next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts" ]
}
