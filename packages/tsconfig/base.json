{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Default",
	"compileOnSave": false,
	"extends": "@tsconfig/node18-strictest-esm",
	"compilerOptions": {
		"exactOptionalPropertyTypes": false,
		"strictPropertyInitialization": false,
		"noUncheckedIndexedAccess": false,
		"importHelpers": true
	},
	"exclude": [ "node_modules", "tmp" ]
}
