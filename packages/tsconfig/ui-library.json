{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "UI Library",
	"extends": "./base.json",
	"compilerOptions": {
		"target": "ESNext",
		"useDefineForClassFields": true,
		"lib": [ "DOM", "DOM.Iterable", "ESNext" ],
		"module": "ESNext",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "react-jsx",
		"types": [ "vite/client" ]
	}
}
