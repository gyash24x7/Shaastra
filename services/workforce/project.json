{
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/services/workforce/src",
	"projectType": "application",
	"targets": {
		"build": {
			"executor": "@nrwl/node:webpack",
			"outputs": [
				"{options.outputPath}"
			],
			"options": {
				"outputPath": "dist/apps/services/workforce",
				"main": "apps/services/workforce/src/main.ts",
				"tsConfig": "apps/services/workforce/tsconfig.app.json",
				"assets": [
					"apps/services/workforce/src/assets"
				]
			},
			"configurations": {
				"production": {
					"optimization": true,
					"extractLicenses": true,
					"inspect": false,
					"fileReplacements": [
						{
							"replace": "apps/services/workforce/src/environments/environment.ts",
							"with": "apps/services/workforce/src/environments/environment.prod.ts"
						}
					]
				}
			}
		},
		"serve": {
			"executor": "@nrwl/node:node",
			"options": {
				"buildTarget": "services-workforce:build"
			},
			"configurations": {
				"production": {
					"buildTarget": "services-workforce:build:production"
				}
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": [
				"{options.outputFile}"
			],
			"options": {
				"lintFilePatterns": [
					"apps/services/workforce/**/*.ts"
				]
			}
		}
	},
	"tags": [ ]
}
