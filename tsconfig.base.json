{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Default",
	"compileOnSave": false,
	"extends": "@tsconfig/node18-strictest",
	"compilerOptions": {
		"exactOptionalPropertyTypes": false,
		"strictPropertyInitialization": false,
		"noUncheckedIndexedAccess": false,
		"ignoreDeprecations": "5.0",
		"importHelpers": true,
		"esModuleInterop": true,
		"rootDir": ".",
		"baseUrl": ".",
		"paths": {
			"@prime/ui": [ "packages/prime/ui/src/index.ts" ],
			"@prime/client": [ "packages/prime/client/src/index.ts" ],
			"@api/common": [ "packages/api/common/src/index.ts" ],
			"@api/domain": [ "packages/api/domain/src/index.ts" ]
		}
	},
	"exclude": [ "node_modules", "tmp", "dist", "coverage" ]
}
