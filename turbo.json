{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"clean": {
			"cache": false
		},
		"build": {
			"dependsOn": [ "db:generate", "schema:generate", "^build" ],
			"outputs": [ "./dist/**" ]
		},
		"dev": {
			"dependsOn": [ "schema:generate", "db:generate", "^build" ],
			"cache": false
		},
		"db:push": {
			"cache": false
		},
		"db:generate": {
			"inputs": [ "./src/prisma/schema.prisma" ],
			"outputs": [ ],
			"cache": true
		},
		"storybook": {
			"cache": false
		},
		"storybook:build": {
			"cache": true
		},
		"client:generate": {
			"inputs": [ "schema/**" ],
			"outputs": [ "./src/generated/**" ],
			"cache": true
		},
		"schema:generate": {
			"cache": true
		}
	}
}
