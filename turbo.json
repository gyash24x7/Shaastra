{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"dev": {
			"inputs": [ "../../../.swcrc", "src/**" ],
			"dependsOn": [ "^build", "db:generate" ],
			"cache": false
		},
		"build": {
			"inputs": [ "../../../.swcrc", "src/**" ],
			"dependsOn": [ "^build", "db:generate" ],
			"outputs": [ "dist/**" ]
		},
		"typecheck": {
			"dependsOn": [ "^typecheck", "db:generate" ],
			"outputs": [ ]
		},
		"start": {
			"dependsOn": [ "db:push", "schema:publish", "build", "typecheck" ],
			"cache": false
		},
		"test": {
			"outputs": [ "coverage/**" ]
		},
		"db:push": {
			"cache": false,
			"inputs": [ "prisma/schema.prisma" ],
			"outputs": [ ]
		},
		"db:generate": {
			"inputs": [ "prisma/schema.prisma" ],
			"outputs": [ "node_module/@prisma/client/**" ]
		},
		"schema:publish": {
			"inputs": [ "src/assets/schema.graphql" ],
			"outputs": [ ],
			"env": [ "APOLLO_KEY", "APOLLO_GRAPH_REF" ]
		}
	}
}
