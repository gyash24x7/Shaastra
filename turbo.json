{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"dev": {
			"dependsOn": [ "db:generate", "gql:generate", "build" ],
			"cache": false
		},
		"build": {
			"dependsOn": [ "^build", "db:generate", "gql:generate" ],
			"outputs": [ "dist/**" ]
		},
		"typecheck": {
			"dependsOn": [ "build", "^typecheck" ],
			"outputs": [ ]
		},
		"start": {
			"dependsOn": [ "build", "typecheck" ],
			"cache": false
		},
		"db:push": {
			"inputs": [ "prisma/schema.prisma" ],
			"outputs": [ ]
		},
		"db:generate": {
			"inputs": [ "prisma/schema.prisma" ],
			"outputs": [ ]
		},
		"gql:generate": {
			"inputs": [ "src/graphql/schema.graphql" ],
			"outputs": [ "src/graphql/generated" ]
		},
		"@shaastra/gateway#gql:generate": {
			"inputs": [ "supergraph-compose.yml" ],
			"dependsOn": [
				"@shaastra/identity#gql:generate",
				"@shaastra/connect#gql:generate",
				"@shaastra/workforce#gql:generate"
			]
		}
	}
}
